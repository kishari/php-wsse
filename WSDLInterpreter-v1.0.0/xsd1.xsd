  <!-- - - - - ComplexTypeSection - - - -->

  <!-- Type "allowanceIdentified" -->
  <complexType name="allowanceIdentified">
    <simpleContent>
      <extension base="tns:allowance">
        <attribute name="id" type="ID" use="required" />
      </extension>
    </simpleContent>
  </complexType>

  <!-- Type "bmCheckMsgListIdentified" -->
  <complexType name="bmCheckMsgListIdentified">
    <sequence>
      <element maxOccurs="9" name="bmCheckMsg" type="nst:string1x70Identified" />
    </sequence>
    <attribute name="id" type="ID" use="required" />
  </complexType>

   <!-- Type "bmMessageList" -->
   <complexType name="bmMessageList">
       <sequence>
           <element maxOccurs="unbounded" name="bmMessage" type="tns:bmMessage" />
       </sequence>
   </complexType>

   <!-- Type "bmMessage" -->
   <complexType name="bmMessage">
       <all>
           <element name="severity" type="nst:severity" />
           <element name="description" type="nst:string1x200" />
           <element minOccurs="0" name="oldValue" type="nst:string1x200" />
           <element minOccurs="0" name="newValue" type="nst:string1x200" />
           <element minOccurs="0" name="fieldCode" type="nst:string1x70" />
       </all>
   </complexType>

